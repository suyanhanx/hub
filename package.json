{
  "name": "wakatime-sync",
  "version": "1.0.1",
  "description": "wakatime api data sync",
  "main": "index.js",
  "scripts": {
    "start": "npm run serve",
    "serve": "NODE_ENV=production pm2 start pm2.json --env production",
    "dev": "NODE_ENV=development babel-node build/index.js",
    "build": "rm -rf build && npm run ts-lint && npm run build-ts",
    "build-docker": "DOCKER_ENV=docker && npm run build",
    "images": "docker build -t wakatime-sync .",
    "build-ts": "tsc",
    "watch-node": "nodemon build/index.js",
    "watch-ts": "tsc -w",
    "ts-lint": "tslint -c tslint.json -p tsconfig.json"
  },
  "author": "superman",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.18.1",
    "moment": "^2.22.2",
    "node-schedule": "^1.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/node": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/runtime": "^7.2.0",
    "@types/axios": "^0.14.0",
    "@types/moment": "^2.13.0",
    "@types/node": "^10.12.18",
    "babel-eslint": "^8.2.6",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.4.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-prettier": "^2.6.2",
    "tslint": "^5.12.0",
    "typescript": "^3.2.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
